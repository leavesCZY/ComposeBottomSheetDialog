<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.ComposeBottomSheetDialog" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:statusBarColor">@color/color_transparent</item>
        <item name="android:navigationBarColor">@color/color_transparent</item>
        <item name="android:enforceStatusBarContrast" tools:ignore="NewApi">false</item>
        <item name="android:enforceNavigationBarContrast" tools:ignore="NewApi">false</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:windowLightNavigationBar" tools:targetApi="o_mr1">true</item>
    </style>

</resources>